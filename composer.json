{
	"require": {
		"radishconcepts/naguro-connector-library": "dev-master",
		"radishconcepts/naguro-frontend-assets": "dev-master",
		"xrstf/composer-php52": "1.*"
	},
	"repositories": [
		{
			"type": "vcs",
			"url":  "git@github.com:radishconcepts/naguro-connector-library.git"
		},
		{
			"type": "vcs",
			"url":  "git@github.com:radishconcepts/naguro-frontend-assets.git"
		}
	],
	"scripts": {
		"post-install-cmd": [
			"xrstf\\Composer52\\Generator::onPostInstallCmd"
		],
		"post-update-cmd": [
			"xrstf\\Composer52\\Generator::onPostInstallCmd"
		],
		"post-autoload-dump": [
			"xrstf\\Composer52\\Generator::onPostInstallCmd"
		]
	},
	"autoload": {
		"classmap": [
			"includes/admin",
			"includes/core"
		]
	}
}